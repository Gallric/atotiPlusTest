	    {
	    	"sub_folder_name": "${FX_subdir}",
	    	"type": "XML.Sensitivities",
	    	"prefix": "${batch_id}",
	    	"parser": {"id": "Output.Sensitivities", "function": "parse_values_per_counterparty"},
	    	"rename_scenario": "^(\\w*)",
	    	"new_measures": {
                "pnl_stock_cva_bilateral_contingent": "-(contingent_cva - contingent_dva)",
                "pnl_fva_risk_free": "-(risky_funding_cost - 1 * risky_funding_benefit)"
            },
			"filter_list": {
				"FX_UP": "([\\w.]*).UP",
				"FX_DW": "([\\w.]*).DW"
			},
	    	"compute": {
                "${entity}_CVA.csv": {
					"FX": [
						"(FX_UP__pnl_stock_cva_bilateral_contingent - FX_DW__pnl_stock_cva_bilateral_contingent)/0.02",
						"(FX_UP__pnl_stock_cva_bilateral_contingent - Base__pnl_stock_cva_bilateral_contingent)/0.01",
						"(- FX_DW__pnl_stock_cva_bilateral_contingent + Base__pnl_stock_cva_bilateral_contingent)/0.01"
						],
					"FX_GAMMA": [
						"(FX_UP__pnl_stock_cva_bilateral_contingent + FX_DW__pnl_stock_cva_bilateral_contingent - 2 * Base__pnl_stock_cva_bilateral_contingent)/0.01"
						]},
				"${entity}_FVA.csv": {
					"FX": [
						"(FX_UP__pnl_fva_risk_free - FX_DW__pnl_fva_risk_free)/0.02",
						"(FX_UP__pnl_fva_risk_free - Base__pnl_fva_risk_free)/0.01",
						"(-FX_DW__pnl_fva_risk_free + Base__pnl_fva_risk_free)/0.01"
					],
					"FX_GAMMA": [
						"(FX_UP__pnl_fva_risk_free + FX_DW__pnl_fva_risk_free - 2 * Base__pnl_fva_risk_free)/0.01"
					]
				}
			}
	    },
		{
            "sub_folder_name": "Projections",
            "file_name": "${INFL_projection_filename}",
	    	"type": "CSV.Sensitivities",
			"sens_type": "INFL.PROJECTION",
	    	"key": "COUNTERPARTY",
            "index": "COUNTERPARTY",
	    	"pandas_csv_arguments": {"sep": ","},
            "prefix": "${batch_id}",
	    	"new_measures": {
                "pnl_stock_cva_bilateral_contingent": "-(contingent_cva - contingent_dva)",
                "pnl_fva_risk_free": "-(risky_funding_cost - 1 * risky_funding_benefit)"
            },
	    	"values": {
                "${entity}_CVA.csv": ["pnl_stock_cva_bilateral_contingent"],
